@page
@model Navistar2.Pages.myDeptDetailPopUp.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DeptDetailPopUp[0].SLT)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeptDetailPopUp[0].Department)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeptDetailPopUp[0].Department_description)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeptDetailPopUp[0].Finance_Department)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeptDetailPopUp[0].Employee_Record_Number)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeptDetailPopUp[0].First_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeptDetailPopUp[0].Last_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeptDetailPopUp[0].Supervisor_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeptDetailPopUp[0].Job_Title)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeptDetailPopUp[0].Employee)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeptDetailPopUp[0].Contractor)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.DeptDetailPopUp) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SLT)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Department)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Department_description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Finance_Department)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Employee_Record_Number)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.First_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Last_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Supervisor_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Job_Title)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Employee)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Contractor)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.DID">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.DID">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.DID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
